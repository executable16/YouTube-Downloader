   "source": [
    "import pytube\n",
    "print(\"Enter the URL\")\n",
    "link = input()\n",
    "yt = pytube.YouTube(link)\n",
    "stream = yt.streams.first()\n",
    "print(\"Enter Destination \")\n",
    "dest=input()\n",
    "stream.download(dest)\n",
    "print(\"Video Downloaded Successfully!\")\n"
   ]
